    "task": "Named Entity Recognition",
    "deployment": "production",
    "integrationType" : "Integrated",
    "name": "spaCy monolingual and bilingual NER and NEL",
    "logo": "It-Sr-NER-logo-small.png",
    "homepage": "https://github.com/rankastankovic/It-Sr-NER",
    "location": "Belgrade, Serbia",
    "creators": "Universit√† degli studi di Torino",
    "contact": {
        "person": "Olja Perisic",
        "email": "olja.perisic@unito.it"
    },
    "authentication": "no",
    "description": "NER and NEL web service for monolingual (txt) and parallel text (tmx).",
    
    "webApplication" : {
		"url": "https://ners.jerteh.rs/api",
    	"queryParameters": [{
			"name": "lng",
			"bind": "file/language",
			"encoding": "639-1"
        }, { 
          	"name": "with_ner",
			"bind": "with_ner",
			"encoding": "639-1"
       	}, {
          	"name": "with_nel",
			"bind": "with_nel",
			"encoding": "639-1"
        }, {
          	"name": "with_tmx",
			"bind": "with_tmx",
			"encoding": "639-1"
        }, {
          	"name": "file",
			"bind": "file/dataurl",
			"encoding": "639-1"
        }],
          
       
    },
    

     "inputs" : [{
        "id": "file",
        "mediatypes": ["text/plain", "application/xml"],
        "languages": ["cat", "zho", "hrv", "dan",  "nld", "eng", "fin", "fra", "deu", "ell", "ita", "jpn", "kor", "lit", "mkd", "nob", "pol", "por", "ron", "rus", "spa", "swe", "ukr", "srp"]
     }, {
		"id": "with_nel",
		"optional" : true
     }, {
		"id": "with_ner",
      		"optional" : true
	
     }, {
		"id": "with_tmx",
       		"optional" : true
	
     }],
      
}
